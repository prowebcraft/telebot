{
    "name": "prowebcraft/telebot",
    "description": "Telegram bot with dialogs and session management",
    "minimum-stability": "dev",
    "keywords": [
        "php",
        "telegram",
        "bot",
        "bot api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Mistulov",
            "email": "prowebcraft@gmail.com"
        }
    ],
    "type": "package",
    "autoload": {
        "psr-4" : {
            "Prowebcraft\\Telebot\\" : "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/prowebcraft/telegram-api"
        }
    ],
    "require": {
        "prowebcraft/php-json-db": "dev-master",
        "telegram-bot/api": "dev-master",
        "symfony/console": "^7.2",
        "monolog/monolog": "~2.9.3",
        "symfony/translation": "^v7.2.2",
        "ext-curl": "*",
        "ext-json": "*"
    }
}
